{
  "name": "dorg-rep-token",
  "version": "0.1.0-beta",
  "private": true,
  "repository": {
    "url": "git@github.com:dOrgTech/rep-token.git",
    "type": "git"
  },
  "scripts": {
    "prepare": "env-prompt -d .env.sample",
    "build": "yarn clean && yarn compile && yarn typechain",
    "lint": "npx solhint contracts/**/*.sol",
    "clean": "rm -rf artifacts cache coverage && hardhat clean",
    "compile": "hardhat compile",
    "deploy:goerli": "hardhat deploy --network goerli",
    "deploy:mainnet": "hardhat deploy --network mainnet",
    "help": "npx hardhat help",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "typechain": "hardhat typechain"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^6.0.5",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.3.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^9.0.2",
    "env-prompt": "^1.2.3",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "hardhat": "^2.3.0",
    "hardhat-deploy": "^0.7.6",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "mocha": "^8.4.0",
    "solhint": "^3.3.4",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.2.0"
  }
}
